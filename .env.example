# API Keys
PERPLEXITY_API_KEY=your_perplexity_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GITHUB_TOKEN=your_github_token_here
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
NODE_ENV=development
LOG_LEVEL=info
PORT=3000
HOST=localhost

# REST API Configuration
REST_API_ENABLED=false
REST_API_PORT=3000

# HTTP Transport Configuration
HTTP_TRANSPORT_ENABLED=true
HTTP_TRANSPORT_PORT=3001
HTTP_TRANSPORT_PATH=/mcp

# SSE Transport Configuration
SSE_TRANSPORT_ENABLED=false
SSE_TRANSPORT_PORT=3002
SSE_TRANSPORT_PATH=/mcp-sse

# Web Interface Configuration
WEB_INTERFACE_ENABLED=true
WEB_INTERFACE_PORT=3003

# HTTPS Configuration
HTTPS_ENABLED=false
HTTPS_KEY_PATH=path/to/key.pem
HTTPS_CERT_PATH=path/to/cert.pem

# Rate Limiting Configuration
# These values define requests per minute
PERPLEXITY_RATE_LIMIT=20
GEMINI_RATE_LIMIT=60
GITHUB_RATE_LIMIT=300

# Storage Configuration
STORAGE_PATH=./storage
RESEARCH_DIR=local-research

# Rate Limiting
PERPLEXITY_RATE_LIMIT_MAX_TOKENS=20
PERPLEXITY_RATE_LIMIT_REFILL_RATE=0.33
GEMINI_RATE_LIMIT_MAX_TOKENS=60
GEMINI_RATE_LIMIT_REFILL_RATE=1

# Add your API keys and configuration values here
# DO NOT commit actual API keys to version control
# Copy this file to .env and fill in your values 